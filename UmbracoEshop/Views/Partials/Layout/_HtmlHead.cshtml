@inherits UmbracoViewPage
<head>
    @if (System.Configuration.ConfigurationManager.AppSettings["redirectToHttps"] == "true")
    {
        <script>
            if (window.location.protocol != 'https:') {
                location.href = location.href.replace("http://", "https://");
            }
        </script>
    }

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="copyright" content="Eshop-LWS.s.r.o" />
    <meta name="description" content="@Model.Value("metaDescription")" />
    <meta name="language" content="sk" />
    <meta name="content-language" content="sk" />
    <meta name="author" content="Eshop-LWS.s.r.o" />
    <meta name="robots" content="index, noodp" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="shortcut icon" href="@Url.Content("~/Styles/images/LWSlogo.jpg")" />

    <title>@Model.Value("pageTitle")</title>
    <link href="@Url.Content("~/Styles/css/bootstrap.min.css?ver=2")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Styles/css/bootstrap-theme.min.css?v3")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Styles/fontawesome-free-5.13.0/css/all.min.css?v1")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Styles/css/lightslider.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Styles/css/lightgallery.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Styles/css/BootstrapStyleSheet.css?v12")" type="text/css" rel="stylesheet" />


    <script src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")"></script>
    @*<script src="@Url.Content("~/Scripts/jquery-3.6.1.min.js")"></script>*@
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js?v1")"></script>

</head>
